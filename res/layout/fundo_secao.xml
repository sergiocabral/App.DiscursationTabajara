<?xml version="1.0" encoding="UTF-8"?>
<selector
    xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true" >
		<shape 
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:shape="rectangle">
		 
			<gradient
			android:startColor="#FF621300"
			android:endColor="#FF924545"
			android:angle="90"/>
		 
			<corners
			android:bottomRightRadius="10dp"
			android:bottomLeftRadius="10dp"
			android:topLeftRadius="10dp"
			android:topRightRadius="10dp"/>
		 
		 	<stroke android:width="1dp" android:color="#66FFFFFF" /> 
		</shape>
    </item>

    <item android:state_focused="true" >
		<shape 
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:shape="rectangle">
		 
			<gradient
			android:startColor="#99621300"
			android:endColor="#99924545"
			android:angle="90"/>
		 
			<corners
			android:bottomRightRadius="10dp"
			android:bottomLeftRadius="10dp"
			android:topLeftRadius="10dp"
			android:topRightRadius="10dp"/>
		 
		 	<stroke android:width="1dp" android:color="#66FFFFFF" /> 
		</shape>
    </item>

    <item>        
		<shape 
		xmlns:android="http://schemas.android.com/apk/res/android"
		android:shape="rectangle">
		 
			<gradient
			android:startColor="#99002C62"
			android:endColor="#9948688E"
			android:angle="90"/>
		 
			<corners
			android:bottomRightRadius="10dp"
			android:bottomLeftRadius="10dp"
			android:topLeftRadius="10dp"
			android:topRightRadius="10dp"/>
		 
		 	<stroke android:width="1dp" android:color="#66FFFFFF" /> 
		</shape>
    </item>
</selector>